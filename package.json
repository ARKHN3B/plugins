{
  "name": "netlify-plugins",
  "private": true,
  "version": "1.0.0",
  "description": "Netlify plugins",
  "scripts": {
    "lint": "eslint 'scripts/**/*.js'",
    "format": "prettier --write 'scripts/**/*.js'",
    "format:ci": "prettier --check 'scripts/**/*.js'",
    "npm-diff": "node scripts/npm-diff.js"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "netlify-plugin",
    "netlify"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "execa": "^4.0.2",
    "husky": "^4.2.5",
    "node-fetch": "^2.6.0",
    "prettier": "^2.0.5"
  },
  "dependencies": {},
  "engines": {
    "node": ">=13"
  }
}
